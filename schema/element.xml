<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2000/10/XMLSchema">
<element name="ChapitreElement"> 
	<complexType>
		<attribute name="type" use="required"> 
			<simpleType>
				<restriction base="string">
					<enumeration value="numeroDeChapitre"/>
					<enumeration value="titre"/>
					<enumeration value="subtitre"/> 
					<enumeration value="dialogues"/> 
					<enumeration value="paragraph"/> 
				</restriction> 
			</simpleType>
		</attribute> 
	</complexType>
</element> 
</schema>